{
  "lexicon": 1,
  "id": "dev.atpkeyserver.alpha.group.key.versions",
  "defs": {
    "main": {
      "type": "query",
      "description": "List all versions of a group key with their status.",
      "parameters": {
        "type": "params",
        "required": ["group_id"],
        "properties": {
          "group_id": {
            "type": "string",
            "description": "Group identifier"
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": ["versions"],
          "properties": {
            "versions": {
              "type": "array",
              "items": {
                "type": "ref",
                "ref": "dev.atpkeyserver.alpha.defs#keyVersionInfo"
              }
            }
          }
        }
      }
    }
  }
}
