{
  "lexicon": 1,
  "id": "dev.atpkeyserver.alpha.group.key",
  "defs": {
    "main": {
      "type": "query",
      "description": "Get a symmetric group key for the authenticated user.",
      "parameters": {
        "type": "params",
        "required": ["group_id"],
        "properties": {
          "group_id": {
            "type": "string",
            "description": "Group identifier"
          },
          "version": {
            "type": "integer",
            "description": "Optional: specific key version to retrieve"
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "ref",
          "ref": "dev.atpkeyserver.alpha.defs#groupKey"
        }
      }
    }
  }
}
